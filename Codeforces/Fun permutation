#include "bits/stdc++.h"
using namespace std;

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    int t; 
    if(!(cin>>t)) return 0;
    
    while(t--)
    {
        int n; cin>>n;
        vector<int> p(n);
        for(int i=0;i<n;i++) cin>>p[i];
        for(int i=0;i<n;i++)
        {
            int q = n+1 - p[i];
            cout<<q<<(i+1==n?'\n':' ');
        }
    }
    return 0;
}
