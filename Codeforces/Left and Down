#include <bits/stdc++.h>
using namespace std;

#define ll long long

int main() 
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int q;
    cin >> q;

    while(q--)
    {
        ll a,b,k;
        cin>>a>>b>>k;
        ll gcd=__gcd(a,b);
        if(a/gcd<=k  && b/gcd<=k)
            cout<<1;
        else
            cout<<2;
        cout << '\n';
    }
    return 0;
}
